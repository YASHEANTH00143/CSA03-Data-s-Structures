#include<stdio.h>
int main(){
    int i,n;
    printf("enter the no of elements in an array");
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("even no are \n");
    for(i=0;i<n;i++){
        if(a[i]%2==0){
            printf("%d,",a[i]);
        }
    }
    printf("\nodd no are\n");
    for(i=0;i<n;i++){
        if(a[i]%2!=0){
            printf("%d,",a[i]);
        }
    }
    return 0;
}
